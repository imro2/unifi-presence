<?xml version="1.0" encoding="utf-8"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>5</minor>
  </specVersion>
  <actionList>
    <action>
      <name>SetAddress</name>
      <argumentList>
        <argument>
          <name>newAddressValue</name>
          <direction>in</direction>
          <relatedStateVariable>Address</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetAddress</name>
      <argumentList>
        <argument>
          <name>AddressValue</name>
          <direction>out</direction>
          <relatedStateVariable>Address</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetPeriod</name>
      <argumentList>
        <argument>
          <name>newPeriodValue</name>
          <direction>in</direction>
          <relatedStateVariable>Period</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetPeriod</name>
      <argumentList>
        <argument>
          <name>PeriodValue</name>
          <direction>out</direction>
          <relatedStateVariable>Period</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetInvert</name>
      <argumentList>
        <argument>
          <name>newInvertValue</name>
          <direction>in</direction>
          <relatedStateVariable>Invert</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetInvert</name>
      <argumentList>
        <argument>
          <name>InvertValue</name>
          <direction>out</direction>
          <relatedStateVariable>Invert</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetAllowedFailure</name>
      <argumentList>
        <argument>
          <name>newAllowedFailureValue</name>
          <direction>in</direction>
          <relatedStateVariable>AllowedFailure</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetAllowedFailure</name>
      <argumentList>
        <argument>
          <name>AllowedFailureValue</name>
          <direction>out</direction>
          <relatedStateVariable>AllowedFailure</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
	<!-- Start New variables added for Unifi details -->
    <action>
      <name>SetUnifiURL</name>
      <argumentList>
        <argument>
          <name>newUnifiURLValue</name>
          <direction>in</direction>
          <relatedStateVariable>UnifiURL</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetUnifiURL</name>
      <argumentList>
        <argument>
          <name>UnifiURLValue</name>
          <direction>out</direction>
          <relatedStateVariable>UnifiURL</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetUnifiUser</name>
      <argumentList>
        <argument>
          <name>newUnifiUserValue</name>
          <direction>in</direction>
          <relatedStateVariable>UnifiUser</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetUnifiUser</name>
      <argumentList>
        <argument>
          <name>UnifiUserValue</name>
          <direction>out</direction>
          <relatedStateVariable>UnifiUser</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetUnifiPassword</name>
      <argumentList>
        <argument>
          <name>newUnifiPasswordValue</name>
          <direction>in</direction>
          <relatedStateVariable>UnifiPassword</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetUnifiPassword</name>
      <argumentList>
        <argument>
          <name>UnifiPasswordValue</name>
          <direction>out</direction>
          <relatedStateVariable>UnifiPassword</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
	<!-- End New variables added for Unifi details -->
  </actionList>
 <serviceStateTable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>Period</name>
       <dataType>i4</dataType>
       <defaultValue>60</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>Address</name>
       <dataType>string</dataType>
       <defaultValue>Enter an IP or MAC</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>AllowedFailure</name>
       <dataType>i4</dataType>
       <defaultValue>10</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>Invert</name>
       <dataType>boolean</dataType>
       <defaultValue>false</defaultValue>
    </stateVariable>
	<!-- Start New variables added for Unifi details -->
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>UnifiURL</name>
       <dataType>string</dataType>
       <defaultValue>Enter Server and Port for Unifi</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>UnifiUser</name>
       <dataType>string</dataType>
       <defaultValue>Enter Unifi User Name</defaultValue>
    </stateVariable>
    <stateVariable>
       <sendEventsAttribute>no</sendEventsAttribute>
       <name>UnifiPassword</name>
       <dataType>string</dataType>
       <defaultValue>Unifi User Password</defaultValue>
    </stateVariable>
	<!-- End New variables added for Unifi details -->
 </serviceStateTable>
</scpd>

